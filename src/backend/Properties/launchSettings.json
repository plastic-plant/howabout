{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "howabout": {
      "commandName": "Project",
      "commandLineArgs": ""
    },
    "howabout /?": {
      "commandName": "Project",
      "commandLineArgs": "/?"
    },
    "howabout help": {
      "commandName": "Project",
      "commandLineArgs": "help"
    },
    "howabout version": {
      "commandName": "Project",
      "commandLineArgs": "version"
    },
    "howabout start": {
      "commandName": "Project",
      "commandLineArgs": "start",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.SpaProxy",
        "ASPNETCORE_URLS": "http://localhost:5000/"
      }
    },
    "howabout stop": {
      "commandName": "Project",
      "commandLineArgs": "stop"
    },
    "howabout add": {
      "commandName": "Project",
      "commandLineArgs": "add"
    },
    "howabout add document1.pdf": {
      "commandName": "Project",
      "commandLineArgs": "add ./example//document1.txt --tag Alice"
    },
    "howabout add document2.pdf": {
      "commandName": "Project",
      "commandLineArgs": "add ./example//document2.pdf --tag Bob"
    },
    "howabout add folder": {
      "commandName": "Project",
      "commandLineArgs": "add \"R:\\GitHub\\plastic-plant\\howabout\\doc\\examples\\wiki_pages\\Album1.txt\" --tag test4 --tag test5 https://www.google.com/"
    },
    "howabout add url": {
      "commandName": "Project",
      "commandLineArgs": "add https://www.netflix.com/helloworld"
    },
    "howabout add folder url": {
      "commandName": "Project",
      "commandLineArgs": "add small_snips https://www.netflix.com/helloworld"
    },

    "howabout ask <question>": {
      "commandName": "Project",
      "commandLineArgs": "ask Can you give me a summary?"
    },
    "howabout ask \"<question>\" --tag <tag>": {
      "commandName": "Project",
      "commandLineArgs": "ask \"Can you give me a summary?\" --tag test3"
    }
  }
}

