{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "howabout": {
      "commandName": "Project",
      "commandLineArgs": ""
    },
    "howabout /?": {
      "commandName": "Project",
      "commandLineArgs": "/?"
    },
    "howabout help": {
      "commandName": "Project",
      "commandLineArgs": "help"
    },
    "howabout start": {
      "commandName": "Project",
      "commandLineArgs": "start"
    },
    "howabout stop": {
      "commandName": "Project",
      "commandLineArgs": "stop"
    },
    "howabout add": {
      "commandName": "Project",
      "commandLineArgs": "add"
    },
    "howabout add document1.pdf": {
      "commandName": "Project",
      "commandLineArgs": "add ./example//document1.txt --tag Alice"
    },
    "howabout add document2.pdf": {
      "commandName": "Project",
      "commandLineArgs": "add ./example//document2.pdf --tag Bob"
    },
    "howabout ask <question>": {
      "commandName": "Project",
      "commandLineArgs": "ask Can you give me a summary?"
    },
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5153",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.SpaProxy"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7187;http://localhost:5153",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.SpaProxy"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Microsoft.AspNetCore.SpaProxy"
      }
    }
  }
}

